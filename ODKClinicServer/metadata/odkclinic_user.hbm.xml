<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
       "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
       "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.odkclinic.db.model">

    <class name="UserRevisionToken" table="odkclinic_user">

        <id name="id" type="java.lang.String" column="id">
            <generator class="assigned" />
        </id>

        <property name="revisionToken" type="java.util.Date" column="revision_token" not-null="true" />

    </class>
</hibernate-mapping>